<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="317.0" prefWidth="551.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Create_Account_Window_Controller">
    <children>
        <Label layoutX="74.0" layoutY="63.0" text="Create A Username" />
        <Label layoutX="74.0" layoutY="125.0" text="Create A Password" />
        <TextField fx:id="Create_User_Textfield" layoutX="221.0" layoutY="58.0" prefHeight="26.0" prefWidth="234.0" promptText="Create your username" />
        <PasswordField fx:id="Create_Password_Textfield" layoutX="221.0" layoutY="120.0" prefHeight="26.0" prefWidth="234.0" promptText="Create your password" />
        <ButtonBar layoutX="16.0" layoutY="248.0" prefHeight="54.0" prefWidth="518.0">
            <buttons>
                <Button fx:id="Reset_Button" mnemonicParsing="false" onAction="#ResetAction" text="Reset" />
                <Button fx:id="Back_button" minWidth="69.0" mnemonicParsing="false" onAction="#Back_button_pressed" prefHeight="26.0" text="Back" />
                <Button fx:id="Create_button" layoutX="208.0" layoutY="17.0" minWidth="71.0" mnemonicParsing="false" onAction="#Create_button_pressed" prefHeight="26.0" prefWidth="146.0" text="Create" />
            </buttons>
        </ButtonBar>
      <Label layoutX="75.0" layoutY="183.0" text="Confirm your Password" />
      <PasswordField fx:id="Confirm_Password_Textfield" layoutX="221.0" layoutY="178.0" prefHeight="26.0" prefWidth="234.0" promptText="Confirm your password" />
    </children>
</AnchorPane>
