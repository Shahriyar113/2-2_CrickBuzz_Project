<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<DialogPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Update_Score_DialogBox_Controller">
   <content>
      <AnchorPane prefHeight="509.0" prefWidth="647.0">
         <children>
            <Label fx:id="Team_Name_Label" alignment="CENTER" layoutX="155.0" layoutY="43.0" prefHeight="47.0" prefWidth="342.0" text="TEAM NAME" textFill="#3418bf">
               <font>
                  <Font name="Arial Bold" size="22.0" />
               </font>
            </Label>
            <ComboBox fx:id="Select_Batsman_ComboBox" layoutX="52.0" layoutY="135.0" prefHeight="47.0" prefWidth="178.0" promptText="SELECT BATSMAN" />
            <ComboBox fx:id="Select_Bowler_ComboBox" layoutX="408.0" layoutY="135.0" prefHeight="47.0" prefWidth="178.0" promptText="SELECT BOWLER" />
            <ComboBox fx:id="Run_Increment_ComboBox" layoutX="52.0" layoutY="268.0" prefHeight="30.0" prefWidth="158.0" promptText="RUN INCREMENT" />
            <ComboBox fx:id="Bowl_Increment_ComboBox" layoutX="247.0" layoutY="268.0" prefHeight="30.0" prefWidth="158.0" promptText="BOWL INCREMENT" />
            <ComboBox fx:id="Extra_Increment_ComboBox" layoutX="446.0" layoutY="268.0" prefHeight="30.0" prefWidth="158.0" promptText="EXTRA INCREMENT" />
            <Button fx:id="Update_Button" layoutX="52.0" layoutY="375.0" mnemonicParsing="false" onAction="#Update_Button_Clicked" prefHeight="30.0" prefWidth="110.0" text="UPDATE">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Label layoutX="52.0" layoutY="246.0" prefHeight="18.0" prefWidth="133.0" text="RUN UPDATE" />
            <Label layoutX="247.0" layoutY="246.0" prefHeight="18.0" prefWidth="99.0" text="BALL UPDATE" />
            <Label layoutX="446.0" layoutY="246.0" prefHeight="18.0" prefWidth="86.0" text="EXTRA UPDATE" />
            <Label layoutX="52.0" layoutY="107.0" prefHeight="18.0" prefWidth="133.0" text="CURRENT BATSMAN" />
            <Label layoutX="408.0" layoutY="107.0" prefHeight="18.0" prefWidth="133.0" text="CURRENT BOWLER" />
            <ComboBox fx:id="Wicket_ComboBox" layoutX="52.0" layoutY="190.0" prefHeight="30.0" prefWidth="119.0" promptText="WICKET" />
         </children>
      </AnchorPane>
   </content>
   <buttonTypes>
      <ButtonType fx:constant="CLOSE" />
   </buttonTypes>
</DialogPane>
